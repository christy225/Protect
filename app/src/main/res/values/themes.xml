<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Protect" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:spinnerItemStyle">@style/inputFont</item>
        <item name="android:spinnerDropDownItemStyle">@style/inputFont</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TitleFont">
        <item name="android:fontFamily">@font/interbold</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="inputFont" parent="TitleFont">
        <item name="android:color">@color/black</item>
        <item name="android:textSize">19sp</item>
    </style>

    <style name="Theme.Protect" parent="Base.Theme.Protect" />

</resources>